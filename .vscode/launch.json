  "version": "0.2.0",
  "configurations": [
  {
    "type": "node",
    "request": "launch",
    "name": "Gulp deploy",
    "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
    "args": [
      "deploy"
    ]
  },
    {
      "type": "node",
      "request": "launch",
      "name": "CRM UI Watcher",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
}